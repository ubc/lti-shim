{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.4",
    "doctrine/dbal": "^3.0",
    "facade/ignition": "^2.3.6",
    "fakerphp/faker": "^1.4",
    "fideloper/proxy": "^4.0",
    "guzzlehttp/guzzle": "^7.3.0",
    "laravel/fortify": "^1.7",
    "laravel/framework": "^8.0",
    "laravel/sanctum": "^2.8",
    "laravel/tinker": "^2.0",
    "league/uri": "^6.0",
    "league/uri-components": "^2.2",
    "lmc/http-constants": "^1.2",
    "ramsey/uuid": "^4.1",
    "web-token/jwt-easy": "^2.2",
    "web-token/jwt-encryption-algorithm-aesgcm": "^2.2",
    "web-token/jwt-encryption-algorithm-rsa": "^2.2",
    "web-token/jwt-key-mgmt": "^2.2",
    "web-token/jwt-signature-algorithm-rsa": "^2.2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "nunomaduro/collision": "^5.0",
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^9.0"
  },
  "config": {
    "platform": {
      "php": "7.4"
    },
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "UBC\\LTI\\": "lti/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  }
}
